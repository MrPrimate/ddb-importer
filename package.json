{
  "name": "ddb-importer",
  "version": "6.6.50",
  "author": "MrPrimate",
  "description": "Foundry VTT module for integrating D&D Beyond content.",
  "private": true,
  "scripts": {
    "lint": "npx eslint src/",
    "lint-fix": "npx eslint src/ --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
